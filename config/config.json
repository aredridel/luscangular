{

    

    "middleware": {

        "static": {
            "priority": 121,
            "module": {
                "arguments": [ "path:./.build" ]
            }
        }

        ,"router": {
            "module": {
                "arguments": [{ "directory": "path:./controllers" }]
            }
        },

        "angularcsrf": {
            "priority": 111,
            "enabled": true,
            "module": {
                "name": "path:middleware/csrf-for-angular"
            }
        }

        

    }
}
